{
	"name": "facilito-ssr",
	"version": "3.2.0",
	"private": true,
	"engines": {
		"node": ">=16"
	},
	"main": "server.js",
	"scripts": {
		"find:unused": "next-unused",
		"next-dev": "NODE_OPTIONS='--inspect' next dev",
		"strings": "node langServer.js",
		"doc": "typedoc",
		"dev": "npm-run-all --parallel next-dev strings",
		"build": "next build",
		"build:a": "ANALYZE=true npm run build",
		"start": "next start",
		"prod": "firebase emulators:start",
		"preprod": "npm run build",
		"lint": "next lint",
		"predeploy": "npm run build",
		"deploy": "firebase deploy --only hosting,functions:nextServer"
	},
	"next-unused": {
		"alias": {},
		"include": [
			"component"
		],
		"exclude": [],
		"entrypoints": []
	},
