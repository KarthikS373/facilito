{
	"hosting": {
		"target": "release",
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"cleanUrls": true,
		"rewrites": [
			{
				"source": "**",
				"function": "nextServer"
			}
		]
	},
	"functions": {
		"source": ".",
		"predeploy": [],
		"runtime": "nodejs12",
		"ignore": [".firebase/**", ".firebaserc", "firebase.json", "**/node_modules/**"]
	}
}
