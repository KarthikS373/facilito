{
	"hosting": {
		"target": "develop",
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"cleanUrls": true,
		"rewrites": [
			{
				"source": "**",
				"function": "nextServer"
			}
		]
	},
	"functions": {
		"source": ".",
		"predeploy": [],
		"runtime": "nodejs12",
		"ignore": [".firebase/**", ".firebaserc", "firebase.json", "**/node_modules/**", "**/public/**"]
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"emulators": {
		"auth": {
			"port": 9099
		},
		"functions": {
			"port": 5001
		},
		"firestore": {
			"port": 8080
		},
		"hosting": {
			"port": 5000
		},
		"storage": {
			"port": 9199
		},
		"ui": {
			"enabled": true
		}
	},
	"storage": {
		"rules": "storage.rules"
	}
}
